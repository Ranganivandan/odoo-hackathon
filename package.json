{
  "name": "expense-management-backend",
  "version": "1.0.0",
  "description": "Express.js backend for expense management system with approval workflows",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "seed": "node scripts/seed.js",
    "frontend": "cd frontend && npm run dev",
    "start:all": "concurrently \"npm run dev\" \"npm run frontend\" --names \"BACKEND,FRONTEND\" --prefix-colors \"bgBlue.bold,bgMagenta.bold\"",
    "install:all": "npm install && cd frontend && npm install"
  },
  "keywords": [
    "expense",
    "management",
    "approval",
    "workflow",
    "express"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "nodemailer": "^7.0.6",
    "sharp": "^0.32.5",
    "tesseract.js": "^4.1.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  }
}
